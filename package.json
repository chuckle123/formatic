{
  "name": "formatic",
  "version": "1.1.11",
  "description": "Automatic, pluggable form generation",
  "main": "./build/lib/index.js",
  "scripts": {
    "xbuild": "babel lib --out-dir build/lib && rollup -c",
    "build-docs": "next build && next export -o build-docs",
    "docs-publish": "gulp docs-publish",
    "format": "zapier-scripts format '__tests__/**/*.js' 'demo/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "lint": "zapier-scripts lint '__tests__/**/*.js' 'demo/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "live": "node start-server.js",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "xprebuild": "rm -rf build && mkdir -p build/lib",
    "precommit": "zapier-scripts format-staged",
    "xprepublish": "npm run build",
    "start": "node start-server.js",
    "test": "jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zapier/formatic.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "@emotion/core": "10.0.0-beta.5",
    "@zeit/next-css": "^1.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-emotion": "^10.0.0-beta.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^3.3.7",
    "codemirror": "^5.32.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "express": "^4.16.3",
    "jest": "^23.6.0",
    "mz": "^2.7.0",
    "next": "^7.0.2",
    "polished": "^2.3.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-svg-loader": "^2.1.0",
    "react-syntax-highlighter": "^9.0.0",
    "sanitize.css": "^8.0.0",
    "zapier-scripts": "^3.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "deep-equal": "^1.0.1",
    "immutability-helper": "^2.5.1",
    "isomorphic-unfetch": "^2.1.1",
    "object-assign": "^2.1.1",
    "prop-types": "^15.6.0",
    "react-powerplug": "^1.0.0-rc.1",
    "react-scroll-lock": "git+https://github.com/Laiff/react-scroll-lock.git#267bf5bcf84d334aecc20908657088007b698dc2",
    "react-transition-group": "^1.2.1"
  },
  "peerDependencies": {
    "codemirror": ">= 5.32.0",
    "react": ">= 0.16",
    "react-dom": ">= 16.0.0"
  }
}
