{
  "name": "formatic",
  "version": "2.0.0-beta.2",
  "description": "Automatic, pluggable form generation",
  "main": "./build/lib/index.cjs.js",
  "module": "./build/lib/index.esm.js",
  "files": [
    "/build/lib"
  ],
  "scripts": {
    "build-docs-with-api-server": "next build && next export -o build-docs",
    "build-docs": "PORT=`get-port`; yarn start --port $PORT --api-only & wait-on http://localhost:$PORT/api/alive && API_PORT=$PORT yarn build-docs-with-api-server; kill %1",
    "build": "rollup -c",
    "format": "zapier-scripts format '__tests__/**/*.js' 'demo/**/*.js' 'docs/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "lint": "zapier-scripts lint '__tests__/**/*.js' 'demo/**/*.js' 'docs/**/*.js' 'lib/**/*.js' 'scripts/**/*.js' 'tasks/**/*.js' '*.js'",
    "major-release": "yarn version --major && yarn publish && git push --follow-tags",
    "minor-release": "yarn version --minor && yarn publish && git push --follow-tags",
    "patch-release": "yarn version --patch && yarn publish && git push --follow-tags",
    "prebuild": "rm -rf build && mkdir -p build/lib",
    "precommit": "zapier-scripts format-staged",
    "prepublish": "yarn build",
    "publish-docs": "yarn build-docs && touch build-docs/.nojekyll && gh-pages -d build-docs",
    "start": "node start-server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zapier/formatic.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0",
    "@emotion/core": "10.0.0-beta.5",
    "@zeit/next-css": "^1.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-emotion": "^10.0.0-beta.0",
    "codemirror": "^5.32.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.3",
    "get-port-cli": "^2.0.0",
    "gh-pages": "^2.0.1",
    "isomorphic-unfetch": "^2.1.1",
    "jest": "^23.6.0",
    "jest-serializer-html": "^6.0.0",
    "mz": "^2.7.0",
    "next": "^7.0.2",
    "polished": "^2.3.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-powerplug": "^1.0.0",
    "react-svg-loader": "^2.1.0",
    "react-syntax-highlighter": "^9.0.0",
    "rollup": "^1.2.1",
    "rollup-plugin-babel": "^4.3.2",
    "sanitize.css": "^8.0.0",
    "wait-on": "^3.2.0",
    "yargs": "^13.2.1",
    "zapier-scripts": "^3.4.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.2",
    "deep-equal": "^1.0.1",
    "immutability-helper": "^2.5.1",
    "object-assign": "^2.1.1",
    "prop-types": "^15.6.0",
    "react-scroll-lock": "git+https://github.com/Laiff/react-scroll-lock.git#267bf5bcf84d334aecc20908657088007b698dc2",
    "react-transition-group": "^1.2.1"
  },
  "peerDependencies": {
    "codemirror": ">= 5.32.0",
    "react": ">= 0.16",
    "react-dom": ">= 16.0.0"
  },
  "sideEffects": false
}
